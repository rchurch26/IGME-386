            </VisualElement>

			<editor:EnumField label="Type" name="basemap-type-selector" type="Esri.ArcGISMapsSDK.Components.BasemapTypes, ArcGISMapsSDK"/>

        </VisualElement>

    </ScrollView>

</VisualElement>

</UXML>
