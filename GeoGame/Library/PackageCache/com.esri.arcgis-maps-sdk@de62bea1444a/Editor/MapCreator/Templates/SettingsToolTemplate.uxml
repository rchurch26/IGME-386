<?xml version="1.0" encoding="utf-8"?>
<UXML
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="UnityEngine.UIElements"
    xmlns:editor="UnityEditor.UIElements"
    xsi:noNamespaceSchemaLocation="../../../../../UIElementsSchema/UIElements.xsd"
	xsi:schemaLocation="
                        UnityEngine.UIElements ../../../../../../UIElementsSchema/UnityEngine.UIElements.xsd
                        UnityEditor.UIElements ../../../../../../UIElementsSchema/UnityEditor.UIElements.xsd
                        UnityEditor.PackageManager.UI ../../../../../../UIElementsSchema/UnityEditor.PackageManager.UI.xsd"
>

<ScrollView>

	<VisualElement class="tool-holder">

	    <Foldout class="main-foldout" text="Editor Mode">
        
			<Toggle name="toggle-enable-editor-mode" label="Enable Editor Mode"/>
			<Toggle name="toggle-enable-editor-mode-data-fetch" label="Data Fetch with Scene View"/>
			<Toggle name="toggle-enable-editor-mode-rebase" label="Rebase with Scene View"/>

	    </Foldout>

	    <Foldout class="main-foldout" text="Physics">
        
			<Toggle name="toggle-enable-mesh-colliders" label="Enable Mesh Colliders"/>

	    </Foldout>

	</VisualElement>

</ScrollView>

</UXML>
